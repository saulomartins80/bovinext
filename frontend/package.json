{
  "name": "frontend",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env FAST_REFRESH=false node_modules\\.bin\\next dev",
    "build": "node_modules\\.bin\\next build",
    "build:no-lint": "cross-env DISABLE_ESLINT_PLUGIN=true node_modules\\.bin\\next build",
    "build:render": "cross-env DISABLE_ESLINT_PLUGIN=true SKIP_TYPE_CHECK=true NODE_ENV=production TYPESCRIPT_IGNORE_BUILD_ERRORS=true node_modules\\.bin\\next build",
    "build:production": "cross-env DISABLE_ESLINT_PLUGIN=true SKIP_TYPE_CHECK=true NODE_ENV=production node_modules\\.bin\\next build",
    "build:no-check": "cross-env DISABLE_ESLINT_PLUGIN=true SKIP_TYPE_CHECK=true NODE_ENV=production TYPESCRIPT_IGNORE_BUILD_ERRORS=true node_modules\\.bin\\next build",
    "start": "node_modules\\.bin\\next start",
    "render:simulate": "npm run build && npm start",
    "render:simulate:no-lint": "npm run build:no-lint && npm start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:cache": "eslint . --cache",
    "lint:all": "npm run lint && npx prettier --check . && npx stylelint \"**/*.css\"",
    "lint:fix:all": "npm run lint:fix && npx prettier --write . && npx stylelint \"**/*.css\" --fix",
    "format": "prettier --write .",
    "export": "node_modules\\.bin\\next export",
    "format:check": "prettier --check .",
    "stylelint": "stylelint \"**/*.css\"",
    "stylelint:fix": "stylelint \"**/*.css\" --fix",
    "type-check": "tsc --noEmit",
    "build-safe": "npm run build",
    "clean": "rm -rf .next",
    "tailwind": "tailwindcss -i ./tailwind-input.css -o ./tailwind-output.css --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
