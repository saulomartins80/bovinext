    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.16",
    "critters": "^0.0.23",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.3"
  }
