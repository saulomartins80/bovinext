    "next-i18next": "^15.4.2",
    "next-images": "^1.8.5",
    "next-intl": "^4.1.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-chartjs-2": "^5.3.0",
    "react-confetti": "^6.4.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-dom-confetti": "^0.2.0",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-notifications": "^1.7.4",
    "react-paginate": "^8.2.0",
    "react-qr-code": "^2.0.15",
